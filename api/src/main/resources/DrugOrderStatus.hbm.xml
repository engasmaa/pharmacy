<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.icchange.pharmacy">
	
	<class name="DrugOrderStatus" table="pharmacy_druorderstatus" >
		<id name="id" type="int" column="druorderstatus_id" unsaved-value="0">
			<generator class="native">
				<param name="sequence">pharmacy_druorderstatus_id_seq</param>
			</generator>
		</id>
		<discriminator column="id" insert="false" />
		<property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
		<one-to-one name="drugOrder" class="org.openmrs.DrugOrder" column="drug_order_id" />
	</class>


</hibernate-mapping>